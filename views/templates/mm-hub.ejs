<%- include("header") %>
<body>

    <!-- ======= Header ======= -->
    <header id="header" class="d-flex align-items-center ">
      <div class="container-fluid d-flex align-items-center justify-content-lg-between">
        <a href="/" class="logo me-auto me-lg-0"><img src="assets/img/logo.jpg" alt="" class="img-fluid"></a>
        <h1 class="logo me-auto me-lg-0"><a href="/">The Laali Project</a></h1>
  
        <nav id="navbar" class="navbar order-last order-lg-0">
          <ul>
            <% if (des == "Mentor") { %>
              <li><a class="nav-link scrollto" href="/mentor">Dashboard</a></li>
            <% } %>
            <% if (des == "Mentee") { %>
              <li><a class="nav-link scrollto" href="/mentee/getdata">Profile</a></li>
              <li><a class="nav-link scrollto " href="/mentee/getMentorDetails">Mentor Assigned</a></li>
            <% } %>
            <li><a class="nav-link scrollto active" href="/mm-hub">Opportunity Hub</a></li>
            <li><a class="nav-link scrollto " href="/logout">Log Out</a></li>
          </ul>
          <i class="bi bi-list mobile-nav-toggle"></i>
        </nav><!-- .navbar -->
  
        <div class="header-social-links d-flex align-items-center">
          <a href="https://twitter.com/thelaaliproject?lang=en" class="twitter"><i class="bi bi-twitter"></i></a>
          <a href="https://www.facebook.com/thelaaliproject/" class="facebook"><i class="bi bi-facebook"></i></a>
          <a href="https://www.instagram.com/thelaaliproject/" class="instagram"><i class="bi bi-instagram"></i></a>
          <a href="https://www.linkedin.com/company/the-laaliproject/?originalSubdomain=in" class="linkedin"><i class="bi bi-linkedin"></i></i></a>
        </div>
  
      </div>
    </header><!-- End Header -->
   
  <!-- ======= alumni Section ======= -->
  <section id="alumni" class="alumni section-bg">
      <div class="container">
  
        <div class="section-title">
          <h2>Welcome to the Opportunity Hub!</h2>
          <p> Working to bring the impact we want to see. </p>
      </p>
        </div>
  
        <div class="row">
  
          <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
            <div class="member">
              <div class="member-img">
                  <a href="/mm-job"><img src="assets/img/mm.jpeg" class="img-fluid" alt=""></a>
              </div>
              <div class="member-info">
                <h4>Jobs</h4>
              </div>
            </div>
          </div>
  
          <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
            <div class="member">
              <div class="member-img">
                  <a href="/mm-blog"><img src="assets/img/oopor.jpg" class="img-fluid" alt=""></a>
              </div>
              <div class="member-info">
                <h4>Blogs</h4>
              </div>
            </div>
          </div>
  
          <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
            <div class="member">
              <div class="member-img">
                  <a href="/mm-event"><img src="assets/img/alumn.jpg" class="img-fluid" alt=""></a>
              </div>
              <div class="member-info">
                <h4>Events</h4>
              </div>
            </div>
          </div>
  
        </div>
  
      </div>
    </section><!-- End alumni Section -->


    
      <!-- ======= Footer ======= -->
     <footer id="footer">
      <div class="container footer-bottom clearfix">
        <div class="copyright">
          &copy; Copyright <strong><span>TheLaaliProject</span></strong>. All Rights Reserved
        </div>
        <div class="credits">
        </div>
      </div>
    </footer><!-- End Footer -->
  
    <div id="preloader"></div>
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>
  
    <!-- Vendor JS Files -->
    <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="assets/vendor/php-email-form/validate.js"></script>
    <script src="assets/vendor/purecounter/purecounter.js"></script>
    <script src="assets/vendor/swiper/swiper-bundle.min.js"></script>
  
    <!-- Template Main JS File -->
    <script src="assets/js/main.js"></script>
  
  </body>
  
  </html>
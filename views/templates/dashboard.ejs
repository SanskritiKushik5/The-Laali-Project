      <div class="row">
        <div class="column" style="background-color:#fff;">
          <h2>Mentees </h2>
          <fieldset id="group1" class="column">
            <% for(var i=0; i < dataOfMentee.length; i++) { %>
              
                <input type="radio" id="html" name="menteeEmail" value="<%= dataOfMentee[i].email %>" />

<%- include("header") %>

<body>

  <!-- ======= Header ======= -->
  <header id="header" class="d-flex align-items-center ">
    <div class="container-fluid d-flex align-items-center justify-content-lg-between">
      <a href="/" class="logo me-auto me-lg-0"><img src="assets/img/logo.jpg" alt="" class="img-fluid"></a>
      <h1 class="logo me-auto me-lg-0"><a href="/">The Laali Project</a></h1>

      <nav id="navbar" class="navbar order-last order-lg-0">
        <ul>
          <!-- <li><a class="nav-link scrollto " href="myindex.html">Home</a></li> -->
          <li><a class="nav-link scrollto " href="/">Home</a></li>
          <li><a class="nav-link scrollto " href="/mentor">Dashboard</a></li>
          <li><a class="nav-link scrollto " href="/mentor/schedule">Schedule</a></li>
          <li><a class="nav-link scrollto " href="#oopor">Opportunity Hub</a></li>
          <li><a class="nav-link scrollto " href="/logout">Log Out</a></li>
        </ul>
        <i class="bi bi-list mobile-nav-toggle"></i>
      </nav><!-- .navbar -->

      <div class="header-social-links d-flex align-items-center">
        <a href="https://twitter.com/thelaaliproject?lang=en" class="twitter"><i class="bi bi-twitter"></i></a>
        <a href="https://www.facebook.com/thelaaliproject/" class="facebook"><i class="bi bi-facebook"></i></a>
        <a href="https://www.instagram.com/thelaaliproject/" class="instagram"><i class="bi bi-instagram"></i></a>
        <a href="https://www.linkedin.com/company/the-laaliproject/?originalSubdomain=in" class="linkedin"><i class="bi bi-linkedin"></i></i></a>
      </div>

    </div>
  </header><!-- End Header -->
 

<!-- ======= alumni Section ======= -->
<section id="alumni" class="alumni section-bg">
    <div class="container">

      <div class="section-title">
        <h2>Mentee Details </h2>
        <p> Working to bring the impact we want to see. </p>
    </p>
      </div>



      <div class="row">
        <div class="col-lg-4">
          <img src="assets/img/testimonials/testimonials-2.jpg" class="img-fluid" alt="">
          
        </div>
        <div class="col-lg-8 pt-4 pt-lg-0 content">
          <h3><%= mentorData['name'] %></h3>
          <h5>Designer, Organisation</h5>
          

          <div class="row">
            <div class="col-lg-6">
              <ul>
                <li><i class="bi bi-chevron-right"></i> <strong>Email:</strong> <span><%= data['email'] %></span></li>
                <li><i class="bi bi-chevron-right"></i> <strong>Phone No:</strong> <span><%= data['phone'] %></span></li>
                <li><i class="bi bi-chevron-right"></i> <strong>Qualification:</strong> <span><%= data['qualification'] %></span></li>
              </ul>
            </div>
            <div class="col-lg-6">
              <ul>
                <li><i class="bi bi-chevron-right"></i> <strong>Age Group</strong>:</strong> <span><%= data['ageGroup'] %></span></li>
                <li><i class="bi bi-chevron-right"></i> <strong>Designation:</strong> <span><%= data['designation'] %></span></li>
                
              </ul>
            </div>
          </div>
        </div>
      </div>

     

      

    </div>
    
  </section><!-- End alumni Section -->
   <!-- ======= Footer ======= -->
   <footer id="footer">
    <div class="container footer-bottom clearfix">
      <div class="copyright">
        &copy; Copyright <strong><span>TheLaaliProject</span></strong>. All Rights Reserved
      </div>
      <div class="credits">
      </div>
    </div>
  </footer><!-- End Footer -->

  <div id="preloader"></div>
  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
